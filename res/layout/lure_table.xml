<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="horizontal">
		<TextView android:id="@+id/lure_brand" xmlns:android="http://schemas.android.com/apk/res/android"
    		android:layout_width="wrap_content"
    		android:layout_height="wrap_content"/>
		<TextView android:id="@+id/lure_model" xmlns:android="http://schemas.android.com/apk/res/android"
    		android:layout_width="wrap_content"
    		android:layout_height="wrap_content"/>
		<TextView android:id="@+id/lure_category" xmlns:android="http://schemas.android.com/apk/res/android"
    		android:layout_width="wrap_content"
    		android:layout_height="wrap_content"/>
		<TextView android:id="@+id/lure_size" xmlns:android="http://schemas.android.com/apk/res/android"
    		android:layout_width="wrap_content"
    		android:layout_height="wrap_content"/>
		<TextView android:id="@+id/lure_color" xmlns:android="http://schemas.android.com/apk/res/android"
    		android:layout_width="wrap_content"
    		android:layout_height="wrap_content"/>
		<TextView android:id="@+id/lure_species" xmlns:android="http://schemas.android.com/apk/res/android"
    		android:layout_width="wrap_content"
    		android:layout_height="wrap_content"/>
</LinearLayout>